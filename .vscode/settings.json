{
  "terminal.integrated.profiles.linux": {
    "bash-with-activate": {
      "path": "bash",
      "args": ["--rcfile", "${workspaceFolder}/.vscode/bashrc_with_activate"]
    }
  },
  "terminal.integrated.defaultProfile.linux": "bash-with-activate",
  "files.associations": {
    "*.ejs": "html",
    "thread.h": "c",
    "*.inc": "c",
    "compare": "c",
    "deque": "c",
    "string": "c",
    "vector": "c",
    "memory_resource": "c",
    "functional": "c",
    "memory": "c",
    "ostream": "c",
    "string_view": "c",
    "tuple": "c",
    "type_traits": "c",
    "typeinfo": "c",
    "algorithm": "c"
  }
}